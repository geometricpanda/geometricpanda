{
  "npmScope": "geometricpanda",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "geometricpanda": {
      "tags": [],
      "implicitDependencies": [
        "storybook-addon-badges",
        "storybook-addon-iframe"
      ]
    },
    "storybook-addon-badges": { "tags": [] },
    "storybook-addon-iframe": { "tags": [] }
  }
}
